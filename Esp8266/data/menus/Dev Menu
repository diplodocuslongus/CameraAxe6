caSetInfoString("Plug the sound module into port 1.");

caStartMenuMode("Dev Menu");
caTextStatic("This is a test menu");
caTextDynamic("id0", "Dynamic text", "777");
caButton("id1", "This is a button", "Button");
caCheckBox("id2", 0, "This is a checkbox");
caDropSelect("id3", 0, "This is a drop select", "no~yes~maybe");
caDropSelectImmediate("id4", 1, "Drop select immediate", "one~two")
caEditNumber("id5", 999999, 2, "This is an edit number");
caEditNumber2("id6", 9999, 50, "id7", 5000, 8, "This is edit2 number");
caTimeBox("id8", 55, 23, 59, 58, 800, 801, "This is a timebox");
caSlider("id9", 255, 10, "This is a slider");
caEndMenu();

caStartPhotoMode();
caTextStatic("Photo Mode");
caEndMenu();
