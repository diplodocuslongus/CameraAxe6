caSetInfoString("Plug the beam detect module into port 1 and beam emit module into port 2.");

caStartMenuMode("Beam Menu");
caDropSelect("id0", 0, "Trigger On:", "Low~High");
caTextDynamic("id1", "Current Beam Signal", "?");
caEndMenu();

caStartPhotoMode();
caTextStatic("Ready to take your photo!");
caEndMenu();